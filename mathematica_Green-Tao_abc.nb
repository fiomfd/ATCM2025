Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
     "computing", " ", "the", " ", "stopping", " ", "step", " ", "for", " ", 
      "the", " ", "Collatz", " ", "sequence", " ", "with", " ", "the", " ", 
      "initial", " ", "data", " ", "m"}], "=", "1"}], ",", "...", ",", 
    "500"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"collatz", "[", "n_", "]"}], ":=", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"EvenQ", "[", "n", "]"}], ",", 
      RowBox[{"n", "/", "2"}], ",", 
      RowBox[{
       RowBox[{"3", " ", "n"}], "+", "1"}]}], "]"}]}], "\[IndentingNewLine]", 
   "\n", 
   RowBox[{
    RowBox[{"stoppingTime", "[", "m_", "]"}], ":=", 
    RowBox[{
     RowBox[{"Length", "[", 
      RowBox[{"NestWhileList", "[", 
       RowBox[{"collatz", ",", "m", ",", 
        RowBox[{
         RowBox[{"#", ">", "1"}], "&"}]}], "]"}], "]"}], "-", "1"}]}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"ListPlot", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"m", ",", 
         RowBox[{"stoppingTime", "[", "m", "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", "500"}], "}"}]}], "]"}], ",", 
     RowBox[{"PlotStyle", "->", 
      RowBox[{"PointSize", "[", "Small", "]"}]}], ",", 
     RowBox[{"AxesLabel", "->", 
      RowBox[{"{", 
       RowBox[{"\"\<Initial value\>\"", ",", "\"\<Steps to 1\>\""}], 
       "}"}]}]}], "]"}], "\n"}]}]], "Input",
